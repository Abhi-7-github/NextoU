generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Plumber{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}

model Electrician{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}

model Carpenter{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}
model Air_Conditioning{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}

model Painter{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}

model house_hold{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  Rating Float
}



